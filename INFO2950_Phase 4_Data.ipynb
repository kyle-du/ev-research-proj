    "ocm_us_df[\"StateOrProvince\"] = ocm_us_df[\"StateOrProvince\"].map(state_map)\n",
    "ocm_us_df = ocm_us_df[ocm_us_df[\"StateOrProvince\"].isin(state_map.keys())]\n",
